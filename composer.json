{
    "name": "panlatent/craft-geetest",
    "description": "Geetest plugin for Craft 3",
    "type": "craft-plugin",
    "version": "0.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "geetest",
        "oss"
    ],
    "support": {
        "docs": "https://github.com/panlatent/craft-geetest/blob/master/README.md",
        "issues": "https://github.com/panlatent/craft-geetest/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent@gmail.com",
            "homepage": "https://panlatent.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
          "panlatent\\craft\\geetest\\": "src/"
        }
    },
    "extra": {
        "name": "Geetest",
        "handle": "geetest",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/panlatent/craft-geetest/master/CHANGELOG.md",
        "class": "panlatent\\craft\\geetest\\Plugin"
    }
}
